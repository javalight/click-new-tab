{
    "manifest_version": 3,
   
    "name": "Open Links In New Tab",
    "version": "0.0.1",
    "icons": { "512": "icon.png"},
    "description": "Opens all links in a new tab",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "storage"
    ],
    "content_scripts": [{
      "js": ["content.js"],
      "matches": ["*://*/*"]
    }]
  }